@model HackatonWeb.Feature.Map.Models.Mate

<div class="team-mate">
    @Model.FullName
    <div class="social">
        @if (!string.IsNullOrEmpty(Model.LinkedIn))
        {
            <a href="@Model.LinkedIn" class="linkedin">
                <img src="./assets/images/linkedin.png" alt="">
            </a>
        }

        @if (!string.IsNullOrEmpty(Model.Twitter))
        {
            <a href="@Model.Twitter" class="twitter">
                <img src="./assets/images/twitter.png" alt="">
            </a>
        }
    </div>
</div>